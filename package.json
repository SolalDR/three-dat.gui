  "description": "A package which create THREE.js controls on Dat.GUI",
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run docs && rollup -c",
    "dev": "run-p dev:**",
