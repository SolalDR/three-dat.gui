    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "parcel build dev/index.html -d ./docs --public-url https://solaldr.github.io/three-dat.gui/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SolalDR/three-dat.gui.git"
  },
